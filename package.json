{
	"name": "superpowers",
	"displayName": "superpowers",
	"description": "Edit your document with JavaScript functions",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.superpowers",
		"onCommand:extension.superpresets",
		"onCommand:extension.supersorts"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Superpowers configuration",
			"properties": {
				"superpowers.configString": {
					"type": "String",
					"default": ":D",
					"description": "Test config setting. TODO: remove this!"
				}
			}
		},
		"commands": [
			{
				"command": "extension.superpowers",
				"title": "Superpowers"
			}, {
				"command": "extension.superpresets",
				"title": "Superpresets"
			}, {
				"command": "extension.supersorts",
				"title": "Supersorts"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
